# Dokploy DevPod Provider Configuration
# Copy this file to .env and fill in your values

# Required: Dokploy server configuration
DOKPLOY_SERVER_URL=https://your-dokploy-server.com
DOKPLOY_API_TOKEN=your-api-token-here

# Optional: Project organization
DOKPLOY_PROJECT_NAME=devpod-workspaces
DOKPLOY_SERVER_ID=

# Optional: Machine configuration
MACHINE_TYPE=small
MACHINE_IMAGE=ubuntu:22.04

# Optional: DevPod agent configuration
AGENT_PATH=/opt/devpod/agent
